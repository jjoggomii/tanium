<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tt.vr.dao.VrMapper">
		<resultMap type="com.tt.vr.vo.VrVO" id="vrVO">
			<result property ="vrno"		column="VRNO"/>
			<result property ="mbno"		column="MBNO"/>
			<result property ="vrwriter"		column="VRWRITER"/>
			<result property ="dname"		column="DNAME"/>
			<result property ="tname"		column="TNAME"/>
			<result property ="gname"		column="GNAME"/>
			<result property ="pname"		column="PNAME"/>
			<result property ="vrsubject"		column="VRSUBJECT"/>
			<result property ="vrmakedate"		column="VRMAKEDATE"/>
			<result property ="bdstate"		column="BDSTATE"/>
			<result property ="vrtype"		column="VRTYPE"/>
			<result property ="vrstartdate"		column="VRSTARTDATE"/>
			<result property ="vrenddate"		column="VRENDDATE"/>
			<result property ="vrcontent"		column="VRCONTENT"/>
			<result property ="vrinsertdate"		column="VRINSERTDATE"/>
			<result property ="vrupdatedate"		column="VRUPDATEDATE"/>
			<result property ="vrdeleteyn"		column="VRDELETEYN"/>
		</resultMap>
		
		<insert id="insertVr" parameterType="com.tt.vr.vo.VrVO">
			<!--  Mapper - insertVr --> 
				INSERT INTO VRTABLE
												(	 VRNO
													,MBNO
													,VRWRITER
													,DNAME
													,TNAME
													,GNAME
													,PNAME
													,VRSUBJECT
													,VRMAKEDATE
													,BDSTATE
													,VRTYPE
													,VRSTARTDATE
													,VRENDDATE
													,VRCONTENT
													,VRINSERTDATE
													,VRUPDATEDATE
													,VRDELETEYN
												)
									VALUES
												(	 #{vrno}
													,#{mbno}
													,#{vrwriter}
													,#{dname}
													,#{tname}
													,#{gname}
													,#{pname}
													,#{vrsubject}
													,#{vrmakedate}
													,#{bdstate}
													,#{vrtype}
													,#{vrstartdate}
													,#{vrenddate}
													,#{vrcontent}
													,#{vrinsertdate}
													,#{vrupdatedate}
													,#{vrdeleteyn}
												 ) 
		</insert>
</mapper>